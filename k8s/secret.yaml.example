apiVersion: v1
kind: Secret
metadata:
  name: aws-cost-service-secrets
  namespace: monitoring
  labels:
    app: aws-cost-service
    component: credentials
type: Opaque
stringData:
  AWS_ACCESS_KEY_ID: "YOUR_AWS_ACCESS_KEY"
  AWS_SECRET_ACCESS_KEY: "YOUR_AWS_SECRET_KEY"
  KAFKA_USERNAME: ""
  KAFKA_PASSWORD: ""
